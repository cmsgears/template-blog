{
	"name": "cmsgears/template-blog",
	"description": "The blog template for cms module having default theme set to blog.",
	"homepage": "http://www.cmsgears.org", 
	"type": "project",
	"keywords": [ "yii2", "cmsgears", "template", "blog" ],
	"license": "GPLv3",
	"authors": [
		{
			"name": "Bhagwat Singh Chouhan",
			"email": "bhagwat.chouhan@vulpinecode.com",
			"homepage": "http://www.cmsgears.org/",
			"role": "Founder and project lead."
		}
	],
	"support": {
        "source": "http://www.cmsgears.com/templates/blog/downloads",
		"forum": "http://forum.cmsgears.org/",
		"resources": "http://resources.cmsgears.org/",
		"issues": "https://www.amazingdesk.com/cmsgears/issues/",
		"blog": "http://cmsgears.tutorials24x7.com/"
	},
	"minimum-stability": "stable",
    "repositories": [
		{
			"type": "vcs",
			"url": "https://git.vcdevhub.com/cmsgears/module-core.git"
		},
		{
			"type": "vcs",
			"url": "https://git.vcdevhub.com/cmsgears/module-forms.git"
		},
		{
			"type": "vcs",
			"url": "https://git.vcdevhub.com/cmsgears/module-cms.git"
		},
		{
			"type": "vcs",
			"url": "https://git.vcdevhub.com/cmsgears/widget-cleditor.git"
		},
		{
			"type": "vcs",
			"url": "https://git.vcdevhub.com/cmsgears/plugin-file-manager.git"
		}
    ],
    "require": {
        "yiisoft/yii2": "2.0.3",
        "yiisoft/yii2-swiftmailer": "2.0.3",
		"cmsgears/module-core": "dev-dev",
		"cmsgears/module-forms": "dev-dev",
		"cmsgears/module-cms": "dev-dev",
		"cmsgears/widget-cleditor": "dev-dev",
		"cmsgears/plugin-file-manager": "dev-dev"
    },
    "require-dev": {
        "yiisoft/yii2-codeception": "*",
        "yiisoft/yii2-debug": "*",
        "yiisoft/yii2-gii": "*",
        "yiisoft/yii2-faker": "*"
    },
    "extra": {
        "asset-installer-paths": {
            "npm-asset-library": "vendor/npm",
            "bower-asset-library": "vendor/bower"
        }
    }
}