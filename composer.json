{
	"name": "cmsgears/template-blog",
	"description": "The blog template for cms module having default theme set to blog.",
	"homepage": "http://www.cmsgears.org", 
	"type": "project",
	"keywords": [ "yii2", "cmsgears", "template", "blog" ],
	"license": "GPLv3",
	"support": {
        "resources": "http://www.cmsgears.org"
	},
	"minimum-stability": "stable",
    "repositories": [
		{
			"type": "vcs",
			"url": "https://git.vcdevhub.com/cmsgears/widget-newsletter.git"
		},
		{
			"type": "vcs",
			"url": "https://git.vcdevhub.com/cmsgears/widget-tag.git"
		},
		{
			"type": "vcs",
			"url": "https://git.vcdevhub.com/cmsgears/widget-social-share.git"
		},
		{
			"type": "vcs",
			"url": "https://git.vcdevhub.com/cmsgears/widget-blog.git"
		},
		{
			"type": "vcs",
			"url": "https://git.vcdevhub.com/cmsgears/widget-nav-dynamic.git"
		}
		
    ],
    "require": {
        "yiisoft/yii2": "2.0.6",
        "yiisoft/yii2-swiftmailer": "2.0.*",
		"cmsgears/module-core": "dev-1.0.0-alpha",
		"cmsgears/module-forms": "dev-1.0.0-alpha",
		"cmsgears/module-cms": "dev-1.0.0-alpha",
		"cmsgears/plugin-file-manager": "dev-1.0.0-alpha",
		"cmsgears/widget-cleditor": "dev-1.0.0-alpha",
		"cmsgears/widget-nav": "dev-1.0.0-alpha",
		"cmsgears/widget-login": "dev-1.0.0-alpha",
		"cmsgears/widget-gallery": "dev-1.0.0-alpha",
		"cmsgears/widget-newsletter": "dev-dev",
		"cmsgears/widget-tag": "dev-dev",
		"cmsgears/widget-social-share": "dev-dev",
		"cmsgears/widget-blog": "dev-dev",
		"cmsgears/widget-nav-dynamic": "dev-1.0.0-alpha"
    },
    "require-dev": {
        "yiisoft/yii2-codeception": "*",
        "yiisoft/yii2-debug": "*",
        "yiisoft/yii2-gii": "*",
        "yiisoft/yii2-faker": "*"
    },
    "extra": {
        "asset-installer-paths": {
            "npm-asset-library": "vendor/npm",
            "bower-asset-library": "vendor/bower"
        }
    }
}